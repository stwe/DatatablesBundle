{
    "name": "wg/universaldatatablebundle",
    "type": "symfony-bundle",
    "description": "Symfony2 DataTable Bundle For Doctrine2 Entities",
    "keywords": ["symfony2", "datatable", "grid", "table", "datagrid", "pagination"],
    "homepage": "https://github.com/WojciechGrzeszkiewicz/UniversalDataTable",
    "license": "MIT",
    "authors": [
        {
            "name": "stwe"
        },
        {
            "name": "WojciechGrzeszkiewicz"
        }
    ],
    "support": {
        "issues": "https://github.com/WojciechGrzeszkiewicz/UniversalDataTable/issues"
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.6",
        "friendsofsymfony/jsrouting-bundle": "@stable"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "suggest": {
        "components/jquery": "1.11.3",
        "datatables/datatables": "1.10.7",
        "moment/moment": "2.10.2"
    },
    "autoload": {
        "psr-0": { "Wg\\UniversalDataTableBundle": "" }
    },
    "target-dir": "Wg/UniversalDataTableBundle"
}
