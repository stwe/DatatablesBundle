{% if view_features.serverSide %}
    "serverSide": true,
    "ajax": {
        "url": "{{ view_ajax.url }}",
{% if view_ajax.data %}        "data": {{ view_ajax.data }},{% endif %}
        "type": "{{ view_ajax.type }}"
    },
{% else %}
    "serverSide": false,
    "data": {{ view_data|raw }},
{% endif %}
